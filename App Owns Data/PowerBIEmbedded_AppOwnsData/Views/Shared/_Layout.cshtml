    <title>Power BI - Embed Sample</title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="~/Content/style/style.css">  

    <style>
        .row{
width:100%;
display:flex;
}
.col-1{
width:8.33%
}
.col-2{
width:16.66%
}

.col-3{
width:25%
}

.col-4{
width:33.33%
}

.col-5{
width:41.66%
}

.col-6{
width:50%
}

.col-7{
width:58.33%
}

.col-8{
width:66.66%
}

.col-9{
width:75%
}

.col-10{
  width:83.33%
}

.col-11{
width:91.66%
}
.col-12{
  width:100%
}


input[type=text], select {
    width: 100%;
    padding: 12px 20px;
    margin:1px 0px 0px -11px;
    display: inline-block;
    border: 0px solid #ccc;
    border-radius: 4px;
    box-sizing: border-box;
	float:left;
}
button {
    background-color: Transparent;
    background-repeat:no-repeat;
    border: 1px red;
    cursor:pointer;
    overflow: hidden;
    outline:none;
    border-radius: 50%;
    border: 2px solid #ffffff;
}

.pulsating-circle{
  display: none;
}
#controls{
  display: block;
}
*{
margin:0;
padding:0;
}



.pulsating-circle {
  position: absolute;
  transform: translateX(-50%) translateY(-50%);
  width: 15px;
  height: 15px;
  font-family:Segoe UI,arial, verdana,tahoma; font-style:bold; font-size:12px;
}
.pulsating-circle:before {
    content: '';
    position: relative;
    display: block;
    width: 300%;
    height: 300%;
    box-sizing: border-box;
    margin-left: -100%;
    margin-top: -100%;
    border-radius: 45px;
	background-color: #ffffff;
    animation: pulse-ring 1.25s cubic-bezier(0.215, 0.61, 0.355, 1) infinite;
	font-family:Segoe UI,arial, verdana,tahoma; font-style:bold; font-size:12px;
  }

.pulsating-circle:after {
    content: '';
    position: absolute;
    left: 0;
    top: 0;
    display: block;
    width: 100%;
    height: 100%;
    background-color: red;
    border-radius: 15px;
    box-shadow: 0 0 8px rgba(0,0,0,.3);
    animation: pulse-dot 1.25s cubic-bezier(0.455, 0.03, 0.515, 0.955) -.4s infinite;
	font-family:Segoe UI,arial, verdana,tahoma; font-style:bold; font-size:12px;
  }

  #reportContainer iframe{border:0px solid #f00;}
  .speaktext-bin{font-family:Segoe UI,arial, verdana,tahoma; font-style:bold; font-size:12px;}


@@keyframes pulse-ring {
  0% {
    transform: scale(.33);
  }
  80%, 100% {
    opacity: 0;
  }
}

@@keyframes pulse-dot {
  0% {
    transform: scale(.8);
  }
  50% {
    transform: scale(1);
  }
  100% {
    transform: scale(.8);
  }
}
    </style>

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.2.1.min.js"
            integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
            crossorigin="anonymous"></script>
</head>
<body>

    @RenderBody()

    
</body>
</html>
